datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model IdentityEmail {
    userId String @db.Uuid
    email String @id
    @@index([userId])
}

model IdentityOAuth {
    userId String @db.Uuid
    provider String
    subId String
    @@id([provider, subId])
    @@index([userId])
}
